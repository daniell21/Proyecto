				        	<th><%= accountreceivable.profitNumber %></th>
				            <td><%= accountreceivable.client.name %></td>
				            <td><%= accountreceivable.month %></td>
				         	<td><%= accountreceivable.paid%></td>
				          	<td><%= accountreceivable.date %></td>
				          	<td><%= accountreceivable.paid %></td>
				          	<td><%= @rate.name %></td>   
				      </tr>
				    <% end %>
				  	</tbody>
				</table>

			</div>
					

		</div>
		<div class="row">
		
			<div class="col-xs-12">	
				<table class="table table-striped">
					<thead>
				    <tr>
				    <th>Cliente </th>
				    
				      <th>Numero de cuentas</th>
				    
				    </tr>
					</thead>
					<tbody>
					
				    <% @accountreceivables_count.each do |accountreceivable| %>
				    
				    	
