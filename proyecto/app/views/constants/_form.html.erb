<%= simple_form_for(@constant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :monthlyPayment %>
    <%= f.input :installPayment %>
    <%= f.input :completePayment %>
    <%= f.input :reactivationPayment %>
    <%= f.input :tax %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
