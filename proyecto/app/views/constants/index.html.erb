<h1>Tarifas</h1>

  

   
    
<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Monto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @constants.each do |constant| %>
      <tr>
        <td><%= constant.name %></td>
        <td><%= constant.amount %></td>
        <td><%= link_to 'Mostrar', constant, :class => 'btn btn-success btn' %></td>
        <td><%= link_to 'Editar', edit_constant_path(constant), :class => 'btn btn-warning btn'  %></td>
        <td><%= link_to 'Eliminar', constant, method: :delete, data: { confirm: 'EstÃ¡s seguro?' }, :class => 'btn btn-danger btn' %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<div align="center">

</div>
<br>

<div align="right">
  <%= link_to 'Nueva Tarifa', new_constant_path, :class => 'btn btn-default btn'  %>
  </div>
