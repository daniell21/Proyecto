
<h1>Clientes</h1>
<table width="100%" border="2px" cellspacing="0px" cellpadding="8px"
         style="margin:0 auto;">
    <tr>
		<th style="width: 200px;"> Código Profit </th>
        <th style="width: 200px;"> RIF </th>
        <th style="width: 200px;"> Nombre </th>
        <th style="width: 200px;"> País </th>
        <th style="width: 200px;"> Estado </th>
    </tr>
    <%@clients = Client.all%>
    <% @clients.each do |client| %>
       <tr link="<%= edit_client_path(client) %>">
       <td><%= client.code %></td>
        <td><%= client.rif %></td>
        <td><%= client.name %></td>
        <td><%= client.completeCountry%></td>
        <td><%= client.state%></td>
	<%end%>
    <%if @clients.size == 0%>
      No Hay  Clientes
    <%end%>
  </table>